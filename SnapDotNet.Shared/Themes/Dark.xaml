<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:SnapDotNet.Themes">

    <SolidColorBrush x:Key="AppPageBackgroundThemeBrush" Color="#FF1D1D1D"/>
    <SolidColorBrush x:Key="PhoneAppPageBackgroundThemeBrush" Color="#FF000000"/>

    <Style TargetType="TextBlock">
        <Setter Property="Foreground" Value="#FFFAFAFA"/>
    </Style>

</ResourceDictionary>
