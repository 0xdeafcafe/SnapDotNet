<Application
	x:Class="Snapchat.App"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:conversations="using:Snapchat.Converters.Conversations">

	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.ThemeDictionaries>
				<!--<ResourceDictionary x:Key="Dark" Source="Themes/Dark.xaml"/>-->
			</ResourceDictionary.ThemeDictionaries>

			<conversations:SnapIconConverter x:Key="SnapIconConverter" />
			<conversations:SnapColourConverter x:Key="SnapColourConverter" />
			
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="Controls/StateTextBlock.xaml"/>
				<ResourceDictionary Source="Assets/Vectors/SnapStatusIcons.xaml"/>
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Application.Resources>

</Application>