<Application
	x:Class="Snapchat.App"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:converters="using:Snapchat.Converters">
	<Application.Resources>
		<ResourceDictionary>
			<converters:DebugConverter x:Name="DebugConverter" />
			
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="Assets/Vectors/SnapStatusIcons.xaml"/>
				<ResourceDictionary Source="Assets/Vectors/SnapChatIcons.xaml"/>
				<ResourceDictionary Source="Assets/Vectors/UploadConversationStatusIcons.xaml"/>
				<ResourceDictionary Source="Controls/SearchTextBox.xaml"/>
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Application.Resources>
</Application>