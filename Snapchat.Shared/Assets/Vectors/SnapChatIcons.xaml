<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="d">
	<ControlTemplate x:Key="AddFriendIcon">
		<Grid Width="25" Height="23">
			<Border Width="19" Height="10" VerticalAlignment="Bottom" HorizontalAlignment="Left" Background="#FFFFFFFF" CornerRadius="8,8,0,0" />
			<Ellipse Width="12" Height="12" Fill="#FFFFFFFF" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="3.5,1,0,0" />

			<Path Margin="0,1,0,0"
				  Data="M 0,3 L 6,3 M 3,0 L 3,6" Stroke="#FFFFFFFF" StrokeThickness="2" HorizontalAlignment="Right" VerticalAlignment="Top" StrokeStartLineCap="Round" StrokeEndLineCap="Round" StrokeDashCap="Round"/>
		</Grid>
	</ControlTemplate>
</ResourceDictionary>
